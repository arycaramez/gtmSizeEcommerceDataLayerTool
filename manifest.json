{
    "name": "Get Google Tag Manager Size",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "webRequest"
    ],
    "host_permissions": [ "*://*/*" ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Get Google Tag Manager Size"
    },
    "icons": {
        "16": "drink_water16.png",
        "32": "drink_water32.png",
        "48": "drink_water48.png",
        "128": "drink_water128.png"
    }
}